	"Пустое правило": {
		"prefix": ["rule"],
		"body": [
			"rule ${1:ruleName}: ${2:eventName}",
			"",
			"    init {",
			"        \\$labels = \"w_auto\"",
			"    }",
			"",
			"    on ${2:eventName} {",
			"        ",
			"    }",
			""	
		]
	},
	"if else endif": {
		"prefix": ["if else"],
		"body": [
			"if $1 then",
			"    $2",
			"else",
			"    $3",
			"endif",
			""
		]
	},
	"if endif": {
		"prefix": ["if endif"],
